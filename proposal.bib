@INPROCEEDINGS{heirman2008rrappcntb,
  author = {Wim Heirman and Joni Dambre and Dirk Stroobandt and Jan {Van Campenhout}},
  title = {Rent's Rule and Parallel Programs: Characterizing Network Traffic
	
