@INPROCEEDINGS{bienia2008pvsaqcotmbsoc,
  author = {Christian Bienia and Sanjeev Kumar and Kai Li},
  title = {{PARSEC} vs. {SPLASH-2}: A Quantitative Comparison of Two Multithreaded
	
