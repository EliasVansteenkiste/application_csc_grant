@INPROCEEDINGS{kawamura2008nposcicb,
  author = {Yuji Kawamura and Takeshi Yamazaki and Hiroshi Kyusojin and Tatsuya
	Ishiwata and Kazuyoshi Horie},
  title = {Network processing on {SPE} core in {Cell} {Broadband}},
