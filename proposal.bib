@ARTICLE{vassiliadis2004tmpp,
  author = {Vassiliadis, Stamatis and Wong, Stephan and Gaydadjiev, Georgi and
	Bertels, Koen and Kuzmanov, Georgi and Panainte, Elena Moscu},
  title = {The {MOLEN} polymorphic processor},
