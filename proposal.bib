  booktitle = {Proceedings of the 10th European Conference on Parallel Computing
	(Europar 04)},
  year = {2004},
  volume = {3149},
  series = {LNCS},
  pages = {318--327},
  month = {August},
  abstract = {This paper introduces a compiler framework that optimizes embedded
