  title = {{Java} and the Power of Multi-Core Processing},
  booktitle = {CISIS '08: Proceedings of the 2008 International Conference on Complex,
	
	Intelligent and Software Intensive Systems},
  year = {2008},
  pages = {627--631},
  address = {Washington, DC, USA},
